{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "func-names":[0,"always"],
        "react/function-component-definition":[1, {
            "namedComponents": "arrow-function",
            "unnamedComponents":  "arrow-function"
          }]
    }
}
